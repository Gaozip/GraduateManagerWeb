<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.whale.dao.UtilMapper">
	<select id="getDictItemVal" parameterType="string" resultType="string">
		select item_val from sys_dict_item where item_code = '${itemCode}' and is_valid = 1
	</select>
</mapper>